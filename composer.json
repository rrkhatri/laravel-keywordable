{
    "name": "rrkhatri/laravel-keywordable",
    "description": "Add keywords on model and filter models using keywords.",
    "license": "MIT",
    "authors": [
        {
            "name": "Rohan Khatri",
            "email": "khatrirohan80@gmail.com"
        }
    ],
    "homepage": "https://github.com/rrkhatri/laravel-keywordable",
    "keywords": [
        "laravel", "keyword", "keywordable", "rohankhatri", "rrkhatri"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/database": "^8.40",
        "illuminate/support": "~5|~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "RrKhatri\\Keywordable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RrKhatri\\Keywordable\\KeywordServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
